    "node_modules",
    "vscode-extension",
    ".git/objects",
    ".vscode",
    ".vscode-insiders",
    "despoil/*"
  ],
  "cSpell.showAutocompleteSuggestions": true,
  "todohighlight.keywords": [
    {
      "text": " ?? ",
      "color": "#db0",
      "backgroundColor": "rgba(0,0,0,0)",
      "overviewRulerColor": "#970",
      "isWholeLine": true
    },
    {
      "text": " ## ",
      "color": "#9d9",
      "backgroundColor": "rgba(0,0,0,0)",
      "isWholeLine": false
    },
    {
      "text": " #- ",
      "color": "#6b6",
      "backgroundColor": "rgba(0,0,0,0)",
      "isWholeLine": false
    },
    {
      "text": " -# ",
      "color": "#494",
      "backgroundColor": "rgba(0,0,0,0)",
      "isWholeLine": false
    },
    {
      "text": " -- ",
      "color": "#373",
      "backgroundColor": "rgba(0,0,0,0)",
      "isWholeLine": false
    },
    {
      "text": "# ... ",
      "color": "#dd0",
      "backgroundColor": "rgba(0,0,0,0)",
      "overviewRulerColor": "#770",
      "isWholeLine": true
    },
    {
      "text": " ? ",
      "color": "#bbb",
      "backgroundColor": "rgba(255,255,255,0.05)",
      "isWholeLine": true
    },
    {
      "text": "<entity tags>",
      "regex": {
        "pattern": "<.*?>"
      },
      "color": "#59f",
      "backgroundColor": "rgba(0,0,0,0)",
      "isWholeLine": false
    }
  ],
  "editor.wrappingIndent": "deepIndent",
  "editor.wordWrap": "on",
  "editor.foldingStrategy": "indentation",
  "editor.showFoldingControls": "always",
  "files.autoSave": "afterDelay",
  "diffEditor.experimental.useVersion2": true,
  "liveServer.settings.root": "/out/"
